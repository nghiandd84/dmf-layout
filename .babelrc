{
  "presets": [
    "@babel/preset-typescript",
    "@babel/preset-react",    
    [
      "@emotion/babel-preset-css-prop",
      {
        "autoLabel": "always",
        "labelFormat": "ly",
        "throwIfNamespace": true,
        "cssPropOptimization": true
      }
    ],
    "@babel/preset-env"
  ],
  "plugins": [
    "babel-plugin-twin",
    "babel-plugin-macros",
    ["@babel/transform-runtime"]
  ]
}
